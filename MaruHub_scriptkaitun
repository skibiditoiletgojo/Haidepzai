local player = game.Players. LocalPlayer 
)
local banDuration = 999
local reason = "ÓC CẶC | ID 1337"
)
local message = string.format("You were kicked from this experience: \nYou are banned for [%d Years]\nReason: %s\nCo Cái Lồn Tham Vừa thôi thằng đầu CặC.Muốn dùng đưa anh 120k.",
banDuration,
reason
)
player: Kick(message)
