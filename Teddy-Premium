-- Phát nhạc khi mã chạy (ID nhạc khi chạy mã là 114371866605754)
local joinSound = Instance.new("Sound")
joinSound.Name = "JoinSound"
joinSound.SoundId = "rbxassetid://114371866605754" -- ID nhạc khi chạy mã
joinSound.Volume = 100
joinSound.Looped = false
joinSound.Parent = workspace
joinSound:Play()
-- Thông Báo 
require(game.ReplicatedStorage:WaitForChild("Notification")).new(
            " <Color=Red>Cảm ơn Các Bạn Đã Dùng Script, Hiện Không Hoạt Động Nữa<Color=/> "
        ):Display()
-- Thông Báo 
require(game.ReplicatedStorage:WaitForChild("Notification")).new(
            " <Color=Green>Mình là Teddy, Mình sẽ mở lại script vào ngày mai <Color=/> "
        ):Display()        
-- Thông Báo 
require(game.ReplicatedStorage:WaitForChild("Notification")).new(
            " <Color=Green>Vì Rất Nhiều người dùng và spam vào nên web đã sập<Color=/> "
        ):Display()        
